{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket",
        "s3:HeadObject" 
      ],
      "Resource": "*" 
    },
    {
        "Effect": "Allow",
        "Action": [
          "elasticbeanstalk:*",
          "ec2:DeleteNetworkInterface",
          "ec2:DescribeNetworkInterfaces",
          "ec2:CreateNetworkInterface"
      ],
        "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "lambda:GetEnvironmentVariable"
      ],
      "Resource": "*"  
    },
    {
      "Effect": "Allow",
      "Action": "*",
      "Resource": "*"  
    }
  ]
}

